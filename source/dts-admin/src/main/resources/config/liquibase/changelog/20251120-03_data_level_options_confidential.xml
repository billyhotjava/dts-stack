<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.22.xsd">

    <changeSet id="20251120-03-data-level-options-confidential" author="codex">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="system_config"/>
            <sqlCheck expectedResult="1">
                SELECT COUNT(*) FROM system_config WHERE cfg_key = 'user.data-level.options'
            </sqlCheck>
        </preConditions>
        <update tableName="system_config">
            <column name="cfg_value" value="{&quot;options&quot;:[&quot;PUBLIC&quot;,&quot;INTERNAL&quot;,&quot;SECRET&quot;,&quot;CONFIDENTIAL&quot;]}"/>
            <where>cfg_key = 'user.data-level.options'</where>
        </update>
    </changeSet>

</databaseChangeLog>
