<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.22.xsd">

    <changeSet id="20251230-01-drop-user-data-levels" author="admin">
        <preConditions onFail="MARK_RAN">
            <columnExists tableName="admin_keycloak_user" columnName="data_levels"/>
        </preConditions>
        <comment>Remove data_levels column from admin_keycloak_user after retiring user data level attribute</comment>
        <dropColumn tableName="admin_keycloak_user" columnName="data_levels"/>
    </changeSet>

</databaseChangeLog>
