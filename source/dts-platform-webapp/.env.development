# 开发环境配置文件
# API基础URL
VITE_API_BASE_URL=/api

# 是否启用Mock数据 (true/false)
VITE_ENABLE_MOCK=false

# 应用名称
VITE_APP_NAME=数据管理平台

# 应用版本
VITE_APP_VERSION=1.0.0-dev

# 公共路径
VITE_PUBLIC_PATH=/

# 路由模式 (frontend/backend)
VITE_APP_ROUTER_MODE=frontend
VITE_APP_ROUTER_HISTORY=hash

# 默认路由
VITE_APP_DEFAULT_ROUTE=/workbench

# Whether to enable admin UI for managing portal (client) menus.
# Default false: client does not require menu creation API.
VITE_ENABLE_PORTAL_MENU_MGMT=false
