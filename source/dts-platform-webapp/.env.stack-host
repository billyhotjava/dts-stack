# dts-stack host-port mode (bypass Traefik) for platform
# Use when dts-platform is mapped to localhost:18082 -> container 8082

VITE_API_BASE_URL=/api
VITE_API_PROXY_TARGET=http://localhost:18082
# No prefix rewrite; backend expects '/api/**'

VITE_ENABLE_MOCK=false
VITE_PUBLIC_PATH=/
VITE_APP_ROUTER_MODE=frontend
VITE_APP_ROUTER_HISTORY=hash
VITE_APP_DEFAULT_ROUTE=/workbench
VITE_ENABLE_PORTAL_MENU_MGMT=false

