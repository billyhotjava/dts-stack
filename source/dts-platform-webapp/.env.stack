# dts-stack (Traefik) dev mode for platform
# Use when backend is running via dts-stack.yml (Traefik on https://localhost)

VITE_API_BASE_URL=/api
VITE_API_PROXY_TARGET=https://localhost
VITE_API_PROXY_PREFIX=/platform

VITE_ENABLE_MOCK=false
VITE_PUBLIC_PATH=/
VITE_APP_ROUTER_MODE=frontend
VITE_APP_ROUTER_HISTORY=hash
VITE_APP_DEFAULT_ROUTE=/workbench
VITE_ENABLE_PORTAL_MENU_MGMT=false

# Allowed roles to sign in (comma-separated). Empty to allow all authenticated users.
VITE_ALLOWED_LOGIN_ROLES=ROLE_USER,ROLE_OP_ADMIN,ROLE_IAM_ADMIN,ROLE_CATALOG_ADMIN,ROLE_GOV_ADMIN
