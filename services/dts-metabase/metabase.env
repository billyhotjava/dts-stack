# Metabase 环境变量示例（根据环境调整）
MB_DB_CONNECTION_URI=postgres://metabase:metabase@dts-pg:5432/metabase
MB_DB_TYPE=postgres
MB_JETTY_PORT=3000
MB_PLUGINS_DIR=/plugins

# 关闭匿名、自助注册
MB_ENABLE_PUBLIC_SHARING=false
MB_ENABLE_QUERY_CACHING=true
MB_USER_SESSION_MAX_AGE=28800

# OIDC（Keycloak 示例：S10 Realm）
MB_IDENTITY_PROVIDER=oidc
MB_OIDC_CLIENT_ID=metabase
MB_OIDC_CLIENT_SECRET=change-me
MB_OIDC_BASE_URL=https://sso.yuzhicloud.com/realms/S10
MB_OIDC_REDIRECT_URI=https://metabase.example.com/auth/oidc/callback
MB_OIDC_AUTH_URI=/protocol/openid-connect/auth
MB_OIDC_TOKEN_URI=/protocol/openid-connect/token
MB_OIDC_USERINFO_URI=/protocol/openid-connect/userinfo
MB_OIDC_GROUPS_CLAIM=roles
MB_OIDC_ATTRIBUTE_EMAIL=email
MB_OIDC_ATTRIBUTE_FIRSTNAME=given_name
MB_OIDC_ATTRIBUTE_LASTNAME=family_name

# 日志
MB_LOG_FILE=/var/log/metabase/metabase.log
